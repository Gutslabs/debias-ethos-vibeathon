# Backend Environment Variables

# Supabase
SUPABASE_URL=your_supabase_url
SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key

# Twitter (for v1 api support - cookie based)
TWITTER_USERNAME=your_twitter_username
TWITTER_PASSWORD=your_twitter_password
TWITTER_EMAIL=your_twitter_email
TWITTER_CT0=your_ct0_cookie_value
TWITTER_AUTH_TOKEN=your_auth_token_cookie_value

# Twitter API v2 (optional - for additional features)
TWITTER_API_KEY=
TWITTER_API_SECRET_KEY=
TWITTER_ACCESS_TOKEN=
TWITTER_ACCESS_TOKEN_SECRET=

# CoinGecko API (free tier)
COINGECKO_API_KEY=your_coingecko_api_key

# AI API Keys (for call detection with Grok)
XAI_API_KEY=your_xai_api_key
OPENAI_API_KEY=your_openai_api_key

# Server
PORT=3001
NODE_ENV=development

# Optional
PROXY_URL=

